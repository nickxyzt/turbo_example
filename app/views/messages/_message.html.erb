<%= turbo_frame_tag message do %>
  <div style="border: 1px dashed; margin-bottom: 10px;">
    <strong>Body:</strong>
    <%= message.body %>
    <br />
    <%= link_to "Edit this message", edit_message_path(message) %>
    <%= button_to "Destroy this message", message, method: :delete %>
  </div>
<% end %>
